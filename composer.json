{
	"name": "netherphp/avenue",
	"description": "A Request Router.",
	"keywords": [ "php", "router" ],
	"license": "BSD-2-Clause",
	"authors":[
		{ "name": "Bob Majdak Jr",
		  "email": "bob@majdak.net",
		  "homepage": "http://pegasusgate.net/+bob",
		  "role": "developer" }
	],
	"config": {
		"preferred-install": {
			"netherphp/*": "source",
			"*": "dist"
		},
		"allow-plugins": {
			"netherphp/composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php":              "^8.1",
		"netherphp/object": "^4.1",
		"netherphp/console": "^4.0"
	},
	"require-dev": {
		"netherphp/standards": ">=0.1.9",
		"phpunit/phpunit": "^9.5",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
	},
	"bin": [
		"bin/netherave"
	],
	"autoload": {
		"psr-0": {
			"Nether\\":"src/"
		}
	}
}
